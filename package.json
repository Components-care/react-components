{
	"name": "components-care",
	"version": "0.1.0",
	"main": "dist",
	"types": "dist",
	"repository": "https://github.com/Components-care/react-components",
	"license": "Apache-2.0",
	"dependencies": {
		"@date-io/moment": "^1.3.13",
		"@material-ui/core": "^4.11.0",
		"@material-ui/icons": "^4.9.1",
		"@material-ui/pickers": "^3.2.10",
		"@types/node": "^14.6.0",
		"@types/react": "^16.9.46",
		"@types/react-dom": "^16.9.8",
		"@types/react-router-dom": "^5.1.5",
		"@types/react-select": "^3.0.19",
		"formik": "^2.1.5",
		"i18next": "^19.7.0",
		"i18next-browser-languagedetector": "^6.0.1",
		"i18next-resource-store-loader": "^0.1.2",
		"moment": "^2.27.0",
		"react-query": "^2.23.0",
		"react-scripts": "^4.0.0-next.77",
		"react-select": "^3.1.0",
		"typescript": "^4.0.2"
	},
	"peerDependencies": {
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-router-dom": "^5.1.0"
	},
	"scripts": {
		"docgen": "typedoc --exclude 'src/stories/**/*.*'",
		"start": "npm run storybook",
		"prepare": "npm run build",
		"prestart": "install-peers -f",
		"build": "tsc && cpx 'src/assets/**/*' dist/assets",
		"storybook": "start-storybook -p 9009 -s public",
		"build-storybook": "build-storybook -s public",
		"build-ghpages": "bash build-ghpages.sh",
		"deploy-storybook": "storybook-to-ghpages -o storybook-static -s build-ghpages",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"lint-fix": "eslint --fix 'src/**/*.{ts,tsx}'"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged --allow-empty && npm run build"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "eslint --fix",
		"*.{json,md}": "prettier --write"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.11.1",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/preset-env": "^7.11.0",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@storybook/addon-actions": "^6.0.13",
		"@storybook/addon-knobs": "^6.0.13",
		"@storybook/addon-links": "^6.0.13",
		"@storybook/addons": "^6.0.13",
		"@storybook/preset-create-react-app": "^3.1.4",
		"@storybook/react": "^6.0.13",
		"@storybook/storybook-deployer": "^2.8.6",
		"@testing-library/jest-dom": "^5.11.3",
		"@testing-library/react": "^10.4.8",
		"@testing-library/user-event": "^12.1.1",
		"@types/jest": "^26.0.10",
		"cpx": "^1.5.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.2.5",
		"install-peers-cli": "^2.2.0",
		"lint-staged": "^10.2.11",
		"prettier": "^2.0.5",
		"typedoc": "^0.19.0"
	}
}
