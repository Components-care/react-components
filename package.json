{
	"name": "components-care",
	"version": "0.1.0",
	"main": "dist",
	"types": "dist",
	"dependencies": {
		"@date-io/moment": "^1.3.13",
		"@material-ui/core": "^4.11.0",
		"@material-ui/icons": "^4.9.1",
		"@material-ui/pickers": "^3.2.10",
		"@types/node": "^14.0.26",
		"@types/react": "^16.9.43",
		"@types/react-dom": "^16.9.8",
		"@types/react-router-dom": "^5.1.5",
		"@types/react-select": "^3.0.14",
		"i18next": "^19.6.0",
		"i18next-browser-languagedetector": "^5.0.0",
		"i18next-resource-store-loader": "^0.1.2",
		"moment": "^2.27.0",
		"react-scripts": "3.4.1",
		"react-select": "^3.1.0",
		"typescript": "^3.9.6"
	},
	"peerDependencies": {
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-router-dom": "^5.1.0"
	},
	"scripts": {
		"start": "npm run storybook",
		"prepare": "npm run build",
		"prestart": "install-peers -f",
		"build": "tsc && cpx 'src/assets/**/*' dist/assets",
		"storybook": "start-storybook -p 9009 -s public",
		"build-storybook": "build-storybook -s public",
		"lint": "tslint -c tslint.json 'src/**/*.ts*'",
		"lint-fix": "tslint -c tslint.json --fix 'src/**/*.ts*'"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged --allow-empty"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "tslint --fix",
		"*.{json,md}": "prettier --write"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.10.4",
		"@babel/core": "^7.10.4",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@storybook/addon-actions": "^5.3.19",
		"@storybook/addon-knobs": "^5.3.19",
		"@storybook/addon-links": "^5.3.19",
		"@storybook/addons": "^5.3.19",
		"@storybook/preset-create-react-app": "^3.1.4",
		"@storybook/react": "^5.3.19",
		"@types/jest": "^26.0.7",
		"@testing-library/jest-dom": "^5.11.1",
		"@testing-library/react": "^10.4.7",
		"@testing-library/user-event": "^12.0.15",
		"cpx": "^1.5.0",
		"husky": "^4.2.5",
		"install-peers-cli": "^2.2.0",
		"lint-staged": "^10.2.11",
		"prettier": "^2.0.5",
		"tslint": "^6.1.2",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.3.0"
	}
}
